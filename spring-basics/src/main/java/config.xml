<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- bean1 -->
	<bean class="com.dxc.springcore.springbasics.bean.Student" name="student1">
		
		<property name="studentID" value="9876"/>
	
		<property name="studentName">
			<value>Milind</value>
		</property>
		
		<property name="studentAddress">
			<value>Kanke Road Ranchi</value>
		</property>
		
	</bean>
	
	<!-- bean2 -->
	
	<bean class="com.dxc.springcore.springbasics.bean.Student" name="student2" p:studentID="1212" p:studentName="manish" p:studentAddress="Greater Noida" />
	
	
	
</beans>